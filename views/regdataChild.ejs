<script src="/public/javascripts/jquery.js"></script>
<link rel="stylesheet" href="/public/stylesheets/bootstrap.min.css">
<script src="/public/javascripts/bootstrap.min.js"></script>
<link rel="stylesheet" href="/public/stylesheets/regSuccess.css">
<!------ Include the above in your HEAD tag ---------->
<% var formData,image,gender; %>
<div class="container emp-profile">
    <form method="post">
        <div class="row">
            <div class="col-md-4">
                <div class="profile-img" style="width:110%;height:180%;">
                    <img src="/public/uploads/<%- image %>">
                
                </div>
            </div>
            <div class="col-md-6">
                <div class="profile-head">
                            <h5 style="color:green;">
                                Registered Successfully
                            </h5>
                           
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Details</a>
                        </li>
                    </ul>
                </div>
            </div>
            
        </div>
        <div class="row">
            <div class="col-md-4">
            </div>
            <div class="col-md-8">
                <div class="tab-content profile-tab" id="myTabContent">
                    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Name</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.name %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>DOB</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.DOB %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Gender</label>
                                    </div>
                                    <div class="col-md-6">
                                        <% if( formData.gender == 1 ) { gender = "Male";} %>
                                        <%  if( formData.gender == 2 ) { gender = "Female";} %>
                                        <%  if( formData.gender == 3 ) { gender = "Other";} %>
                                        <p><%- gender %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Father's Phone No.</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.Fphoneno %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Mother's Phone No.</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.Mphoneno %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Siblings</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.siblings %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Aadhar No</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.aadharno %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Address Details: </label>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Line 1</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.address[0].line1 %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Line 2</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.address[0].line2 %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>City </label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.address[0].town_village %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Province</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.address[0].province %></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>Pincode</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.address[0].pincode%></p>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>State</label>
                                    </div>
                                    <div class="col-md-6">
                                        <p><%- formData.address[0].state%></p>
                                    </div>
                                </div>
                                <hr>
                    </div>
                    